									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0038, 	-- < 6mån
											at0039]	-- >= 6mån
										}
									}
								}
							}
							ELEMENT[at0015] matches {	-- Inskrivnings-/besöksdatum
								value matches {
									DV_DATE matches {
										value matches {yyyy-mm-dd}
									}
								}
							}
							ELEMENT[at0016] occurrences matches {0..1} matches {	-- Utskrivningsdatum
								value matches {
									DV_DATE matches {
										value matches {yyyy-mm-dd}
									}
								}
							}
						}
					}
				}
			}
		}
	}

