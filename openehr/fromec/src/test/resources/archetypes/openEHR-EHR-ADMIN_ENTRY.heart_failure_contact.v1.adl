											at0050, 	-- Annat
											at0051]	-- Okänt
										}
									}
								}
							}
							ELEMENT[at0013] matches {	-- Tidigare vårdtillfälle/inläggning för hjärtsvikt
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
