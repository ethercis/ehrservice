					text = <"cardinality test">; 
					description = <"most minimal cardinality test">
				>
				["at0001"] = <
					text = <"cluster with cardinality that is too broad for the reference model">; 
					description = <"most minimal cardinality test cluster">
				>
				["at0002"] = <
