definition
	CLUSTER[at0000] matches {	-- *Cluster test data types(en)
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {	-- *freeText(en)
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0015] occurrences matches {0..1} matches {	-- *codedText(en)
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0016, 	-- *text1(en)
							at0017, 	-- *text2(en)
							at0018; 	-- *text3(en)
							at0017]	-- assumed value
						}
					}
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {	-- *quantity(en)
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::124]>
						list = <
							["1"] = <
								units = <"kg">
								magnitude = <|0.0..100.0|>
								precision = <|3|>
							>
							["2"] = <
								units = <"mg">
								magnitude = <|0.0..1000.0|>
							>
						>
					>
				}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {	-- *count(en)
				value matches {
					DV_COUNT matches {
						magnitude matches {|1..15|; 1}
					}
				}
			}
			ELEMENT[at0004] occurrences matches {0..1} matches {	-- *datetime(en)
				value matches {
					DV_DATE matches {
						value matches {yyyy-mm-dd}
					}
				}
			}
			ELEMENT[at0005] occurrences matches {0..1} matches {	-- *duration(en)
				value matches {
					DV_DURATION matches {
						value matches {PDTHMS/|<=PT24H|}
					}
				}
			}
			ELEMENT[at0006] occurrences matches {0..1} matches {	-- *ordinal(en)
				value matches {
					1|[local::at0019], 	-- *entry 1(en)
					2|[local::at0020], 	-- *entry 2(en)
					3|[local::at0021], 	-- *entry 3(en)
					4|[local::at0022], 	-- *entry 4(en)
					5|[local::at0023]  	-- *entry 5(en)
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {	-- *boolean(en)
				value matches {
					DV_BOOLEAN matches {
						value matches {True, False; True}
					}
				}
			}
			ELEMENT[at0008] occurrences matches {0..1} matches {	-- *intervalInteger(en)
				value matches {
					DV_INTERVAL<DV_COUNT> matches {
						upper matches {
							DV_COUNT matches {
								magnitude matches {|<=15|}
							}
						}
						lower matches {
							DV_COUNT matches {
								magnitude matches {|>=5|; 5}
							}
						}
					}
				}
			}
			ELEMENT[at0009] occurrences matches {0..1} matches {	-- *choice(en)
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {[local::]}
					}
				}
			}
			ELEMENT[at0010] occurrences matches {0..1} matches {	-- *image(en)
				value matches {
					DV_MULTIMEDIA matches {
						media_type matches {
							[openEHR::
							425, 
							426, 
							427, 
							428, 
							429]
						}
					}
				}
			}
			ELEMENT[at0011] occurrences matches {0..1} matches {	-- *URI(en)
				value matches {
					DV_URI matches {*}
				}
			}
			ELEMENT[at0012] occurrences matches {0..1} matches {	-- *proportion(en)
				value matches {
					DV_PROPORTION matches {
						numerator matches {|1.0..12.0|; 1.0}
						denominator matches {|1.0..16.0|; 1.0}
					}
				}
			}
			ELEMENT[at0013] occurrences matches {0..1} matches {	-- *ID(en)
				value matches {
					DV_IDENTIFIER matches {*}
				}
			}
			ELEMENT[at0014] occurrences matches {0..1} matches {	-- *parsable(en)
				value matches {
					DV_PARSABLE matches {
						formalism matches {"text/html"}
					}
				}
			}
		}
	}

